{
    "name": "swissfreecommerce/monopay-merchant-laravel",
    "description": "A PHP wrapper for Swiss free commerce payment API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer",
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "ext-curl": "*",
        "ext-json": "*",
        "spatie/laravel-data": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SwissFreeCommerce\\PaymentMerchant\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SwissFreeCommerce\\PaymentMerchant\\Providers\\MonopayPackageServiceProvider"
            ],
            "aliases": {
                "MonopayService": "SwissFreeCommerce\\PaymentMerchant\\MonopayPackageFacade"
            }
        }
    },
    "minimum-stability" : "stable",
    "support": {
        "issues": "https://github.com/swissfreecommerce/payment-merchant/issues",
        "forum": "https://twitter.com/swissfreecommerce",
        "source": "https://github.com/swissfreecommerce/payment-merchant"
    }
}
